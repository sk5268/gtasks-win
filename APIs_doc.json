{
  "name": "tasks",
  "resources": {
    "tasklists": {
      "methods": {
        "delete": {
          "id": "tasks.tasklists.delete",
          "path": "tasks/v1/users/@me/lists/{tasklist}",
          "httpMethod": "DELETE",
          "parameters": {
            "tasklist": { "required": true, "type": "string" }
          }
        },
        "insert": {
          "id": "tasks.tasklists.insert",
          "path": "tasks/v1/users/@me/lists",
          "httpMethod": "POST",
          "request": { "$ref": "TaskList" },
          "response": { "$ref": "TaskList" }
        },
        "list": {
          "id": "tasks.tasklists.list",
          "path": "tasks/v1/users/@me/lists",
          "httpMethod": "GET",
          "response": { "$ref": "TaskLists" }
        }
      }
    },
    "tasks": {
      "methods": {
        "delete": {
          "id": "tasks.tasks.delete",
          "path": "tasks/v1/lists/{tasklist}/tasks/{task}",
          "httpMethod": "DELETE",
          "parameters": {
            "tasklist": { "required": true, "type": "string" },
            "task": { "required": true, "type": "string" }
          }
        },
        "insert": {
          "id": "tasks.tasks.insert",
          "path": "tasks/v1/lists/{tasklist}/tasks",
          "httpMethod": "POST",
          "parameters": { "tasklist": { "required": true, "type": "string" } },
          "request": { "$ref": "Task" },
          "response": { "$ref": "Task" }
        },
        "list": {
          "id": "tasks.tasks.list",
          "path": "tasks/v1/lists/{tasklist}/tasks",
          "httpMethod": "GET",
          "parameters": {
            "tasklist": { "required": true, "type": "string" }
          },
          "response": { "$ref": "Tasks" }
        },
        "patch": {
          "id": "tasks.tasks.patch",
          "path": "tasks/v1/lists/{tasklist}/tasks/{task}",
          "httpMethod": "PATCH",
          "parameters": {
            "tasklist": { "required": true, "type": "string" },
            "task": { "required": true, "type": "string" }
          },
          "request": { "$ref": "Task" },
          "response": { "$ref": "Task" }
        },
        "update": {
          "id": "tasks.tasks.update",
          "path": "tasks/v1/lists/{tasklist}/tasks/{task}",
          "httpMethod": "PUT",
          "parameters": {
            "tasklist": { "required": true, "type": "string" },
            "task": { "required": true, "type": "string" }
          },
          "request": { "$ref": "Task" },
          "response": { "$ref": "Task" }
        }
      }
    }
  },
  "version": "v1"
}